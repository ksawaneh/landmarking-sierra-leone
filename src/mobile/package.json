{
  "name": "landmarking-mobile",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=__tests__/.*\\.test\\.(ts|tsx)$",
    "test:integration": "jest --testPathPattern=__tests__/integration/.*\\.test\\.(ts|tsx)$",
    "test:e2e": "detox test --configuration ios.sim.debug",
    "test:e2e:android": "detox test --configuration android.emu.debug",
    "build:e2e:ios": "detox build --configuration ios.sim.debug",
    "build:e2e:android": "detox build --configuration android.emu.debug",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "expo": "~53.0.10",
    "expo-status-bar": "~2.2.3",
    "react": "19.0.0",
    "react-native": "0.79.3",
    "@react-navigation/native": "^6.1.10",
    "@react-navigation/stack": "^6.3.21",
    "@react-navigation/bottom-tabs": "^6.5.12",
    "react-native-screens": "~3.35.0",
    "react-native-safe-area-context": "4.14.0",
    "expo-location": "~18.0.4",
    "expo-camera": "~16.0.9",
    "expo-local-authentication": "~15.0.1",
    "expo-sqlite": "~15.0.4",
    "@react-native-async-storage/async-storage": "1.25.0",
    "react-native-gesture-handler": "~2.20.2",
    "expo-image-picker": "~16.0.3",
    "expo-file-system": "~18.0.6",
    "expo-network": "~6.0.1",
    "expo-task-manager": "~12.0.2",
    "axios": "^1.6.5",
    "react-hook-form": "^7.48.2",
    "date-fns": "^3.0.6",
    "uuid": "^9.0.1",
    "expo-crypto": "~14.0.1",
    "expo-secure-store": "~14.0.0",
    "react-native-maps": "1.18.0",
    "expo-document-picker": "~13.0.1",
    "react-native-reanimated": "~3.16.5",
    "react-native-vector-icons": "^10.0.3",
    "@react-native-picker/picker": "2.10.2",
    "react-native-signature-canvas": "^4.7.1",
    "react-native-qrcode-svg": "^6.3.0",
    "react-native-svg": "15.8.0",
    "expo-barcode-scanner": "~14.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "typescript": "~5.8.3",
    "@types/react-native": "~0.73.0",
    "@types/uuid": "^9.0.7",
    "jest": "^29.7.0",
    "@testing-library/react-native": "^12.4.2",
    "@testing-library/jest-native": "^5.4.3",
    "jest-expo": "~50.0.1",
    "detox": "^20.13.5",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~7.0.0",
    "@types/jest": "^29.5.11"
  },
  "private": true
}
